<Window x:Class="PublicStore.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PublicStore"
        mc:Ignorable="d"
        xmlns:kategoriak="clr-namespace:PublicStore.ViewModel"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ComboBox ItemsSource="{Binding AruKategoriak}" DisplayMemberPath="AruKategoriaMegnevezes" x:Name="comboBox" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
